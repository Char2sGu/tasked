  "scripts": {
    "prepare": "husky install",
    "release": "standard-version --no-verify",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:client": "ng serve",
    "dev:server": "nest start server -w",
    "dev:libs": "tsc -b tsconfig.libs.json -w",
    "build": "npm run build:libs && concurrently \"npm:build:client\" \"npm:build:server\"",
    "build:client": "ng build",
    "build:server": "nest build server",
    "build:libs": "tsc -b tsconfig.libs.json",
    "test:client": "ng test",
    "test:server": "npm run test:server:unit & npm run test:server:e2e",
    "test:server:unit": "jest --config ./projects/server/jest.config.unit.js",
    "test:server:e2e": "jest --config ./projects/server/jest.config.e2e.js",
    "doc:client:build": "compodoc -d compodoc/client -p projects/client/tsconfig.doc.json ",
    "doc:client:serve": "compodoc -d compodoc/client -s",
    "doc:server:build": "compodoc -d compodoc/server -p projects/server/tsconfig.doc.json ",
    "doc:server:serve": "compodoc -d compodoc/server -s",
    "client:i18n": "ng extract-i18n --format=json --output-path projects/client/src/locales/",
    "server:cli": "ts-node -P projects/server/tsconfig.app.json --files -r tsconfig-paths/register projects/server/src/main.ts",
    "codegen": "graphql-codegen",
    "format": "prettier --write \"{projects/**/*,*}.{html,css,less,scss,sass,ts,js,json,md}\"",
    "lint": "eslint \"projects/**/*.ts\" --cache --fix"
  },
