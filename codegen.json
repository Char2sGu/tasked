{
  "schema": "schema.graphql",
  "documents": "projects/client/src/**/*.graphql",
  "generates": {
    "projects/client/src/app/graphql/codegen.ts": {
      "plugins": [
        "typescript",
        "typescript-operations",
        "typescript-apollo-angular"
      ]
    }
  },
  "config": {
    "addExplicitOverride": true
  }
}
